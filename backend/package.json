{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "pretest:integration": "docker compose down && docker compose --profile test up -d test-db redis",
    "test:integration": "sleep 3 && vitest --run --config vitest.config.integration.ts",
    "posttest:integration": "docker compose --profile test down -v test-db redis",
    "test:unit": "vitest --run --exclude='**/integration/**'",
    "pretest": "docker compose down && docker compose --profile test up -d test-db redis",
    "test": "sleep 3 && vitest --run",
    "posttest": "docker compose --profile test down -v test-db redis",
    "test:ci": "sleep 3 && vitest --run",
    "pretest:ui": "docker compose down && docker compose --profile test up -d test-db redis",
    "test:ui": "sleep 3 && vitest --ui",
    "posttest:ui": "docker compose --profile test down -v test-db redis",
    "test:integration_windows": "powershell -ExecutionPolicy Bypass -File ./integretion-test.ps1",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "db:push": "drizzle-kit push --config drizzle.config.ts",
    "format:check": "prettier . --check",
    "format": "prettier . --write",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "lint": "eslint ./src/",
    "dev": "node --loader=ts-node/esm --env-file=.env --watch src/index.ts",
    "start": "node dist/src/index.js",
    "build": "npx tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@bull-board/api": "^6.4.0",
    "@bull-board/express": "^6.4.0",
    "@bull-board/ui": "^6.4.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.25.6",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.33.0",
    "express": "^4.21.0",
    "heap-js": "^2.5.0",
    "jsonwebtoken": "^9.0.2",
    "lib": "^5.1.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.15",
    "pg": "^8.13.0",
    "postgres": "^3.4.4",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.5",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "2.1.5",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.3.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vitest": "^2.1.5"
  }
}
